end)


MainSection:NewButton("Auto Reroll (Legendary and higher) must have some", "rerolls fast for u", function()
    local WHITELISTED = {"HaÃ±ma", "Kageyama", "Songoku", "Kagutsuchi", "Sung", "Raijin", "Sanji", "Ryu", "Fujin", "Wu", "Kage", "Ohma", "Karu" ,"Akuma", "Mikazuchi", "Kure"}

